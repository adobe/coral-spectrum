{
  "extends": "@adobe/adobe-js",
  "env" : {
    "browser" : true,
    "es6" : true
  },
  "rules": {
    "one-var": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "newline-after-var": 0,
    "guard-for-in": 0,
    "new-cap": 0,
    "padded-blocks": 0,
    "require-jsdoc": 0,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "brace-style": ["error", "stroustrup", { "allowSingleLine": true }],
    "arrow-parens": 0,
    "capitalized-comments": 0,
    "no-param-reassign": 0,
    "valid-jsdoc": 0,
    "max-statements": 0,
    "id-length": 0,
    "no-negated-condition": 0,
    "operator-linebreak": ["error", "after"],
    "max-statements-per-line": 0,
    "no-magic-numbers": 0,
    "newline-per-chained-call": 0,
    "default-case": 0,
    "no-confusing-arrow": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "no-multi-assign": 0,
    "max-depth": 0,
    "max-lines": 1,
    "babel/object-curly-spacing": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}